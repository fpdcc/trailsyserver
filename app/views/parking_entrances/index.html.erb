<p id="notice"><%= notice %></p>

<h1>Listing Parking Entrances</h1>

<table>
  <thead>
    <tr>
      <th>Parking entrance</th>
      <th>Geom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_entrances.each do |parking_entrance| %>
      <tr>
        <td><%= parking_entrance.parking_entrance_id %></td>
        <td><%= parking_entrance.geom %></td>
        <td><%= link_to 'Show', parking_entrance %></td>
        <td><%= link_to 'Edit', edit_parking_entrance_path(parking_entrance) %></td>
        <td><%= link_to 'Destroy', parking_entrance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking entrance', new_parking_entrance_path %>
