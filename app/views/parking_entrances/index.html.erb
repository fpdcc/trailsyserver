
<h1>Listing Parking Entrances</h1>
<table class="table-striped table-bordered table-hover">
  <thead>
    <tr>
      <% for column in ParkingEntrance.column_names %>
        <th><%= column %></th>
      <% end %>
      <th colspan="3">Make Changes</th>
    </tr>
  </thead>
  <tbody>
    <% @parking_entrances.each do |parking_entrance| %>
      <tr>
        <% parking_entrance.attributes.each do |attribute| %>
          <td><%= attribute[1] %></td>
        <% end %>
        <td><%= link_to 'Show', parking_entrance %></td>
        <td><%= link_to 'Edit', edit_parking_entrance_path(parking_entrance) %></td>
        <td><%= link_to 'Destroy', parking_entrance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>
<%= will_paginate @parking_entrances, renderer: BootstrapPagination::Rails %>

<br>

<%= link_to 'New Parking entrance', new_parking_entrance_path %>
