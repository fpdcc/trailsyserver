<p id="notice"><%= notice %></p>

<h1>Listing Trail Subtrails</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trail_subtrails.each do |trail_subtrail| %>
      <tr>
        <td><%= link_to 'Show', trail_subtrail %></td>
        <td><%= link_to 'Edit', edit_trail_subtrail_path(trail_subtrail) %></td>
        <td><%= link_to 'Destroy', trail_subtrail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trail subtrail', new_trail_subtrail_path %>
