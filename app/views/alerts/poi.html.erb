
<p id="notice"><%= notice %></p>

<h1>Alerts#poi</h1>

<%= search_form_for @q, url: request.path, html: { class: 'form-horizontal' } do |f| %>
	<%= f.label :name_cont %>
  	<%= f.search_field :name_cont %>
  	<%= f.label :maintenance_div_cont %>
  	<%= f.select :maintenance_div_cont, Pointsofinterest.maintenance_divs, {include_blank: true}, {:class => "input" } %>
	<%= f.submit class: "btn btn-success" %>
	<%= link_to 'Clear Search', request.path, class: "btn btn-default" %>
<% end %>

<%= render partial: 'table', locals: {items: @pointsofinterests_active, alertable_type: "Pointsofinterest"} %>
<br>
<%= render partial: 'table', locals: {items: @pointsofinterests, alertable_type: "Pointsofinterest"} %>

<%= will_paginate @pointsofinterests, renderer: BootstrapPagination::Rails %>
